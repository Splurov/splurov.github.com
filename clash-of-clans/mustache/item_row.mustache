<tr id="{{rowId}}">
    <td>
        <span class="object-icon __{{id}}"{{^spells}} title="{{title}}"{{/spells}}></span>
        {{#spells}}
            <span class="object-text">{{title}}</span>
        {{/spells}}
    </td>
    <td class="tiny js-col-{{type}}-level">
        <select id="{{id}}-level" class="js-comp-level" data-type="{{type}}">
            {{#levelContent}}
                <option value="{{text}}"{{#selected}} selected="selected"{{/selected}}>{{text}}</option>
            {{/levelContent}}
        </select>
    </td>
    <td class="js-col-{{type}}-quantity">
        <input pattern="[0-9]*"
               data-type="{{type}}"
               class="js-number js-comp-quantity"
               id="{{id}}"
               size="3"
               tabindex="{{tabIndexValue}}"/><!--
        --><button class="button button_spinner js-spinner" tabindex="-1" data-for="{{id}}">+</button><!--
        --><button class="button button_spinner js-spinner" tabindex="-1" data-for="{{id}}">&minus;</button>
    </td>
    <td id="{{id}}-summary" class="number tiny js-col-{{type}}-total">
    </td>
    {{#subtractId}}
        <td class="js-col-{{type}}-subtract">
            <input pattern="[0-9]*"
                   data-type="{{type}}"
                   class="js-number js-comp-subtract"
                   id="{{subtractId}}"
                   size="3"
                   tabindex="{{tabIndexSubtract}}"/><!--
        --><button class="button button_spinner js-spinner" tabindex="-1" data-for="{{subtractId}}">+</button><!--
        --><button class="button button_spinner js-spinner" tabindex="-1" data-for="{{subtractId}}">&minus;</button>
        </td>
    {{/subtractId}}
    {{#barracksTimes}}
        <td id="{{barrackQuantityId}}" class="number js-col-{{type}}-distribution"></td>
    {{/barracksTimes}}
</tr>
