    <tr id="{{rowId}}">
        <td>
            <label for="{{id}}">{{title}}</label>
        </td>
        <td>
            <select id="{{levelId}}" class="field" tabindex="{{tabIndexLevel}}">
                {{#levelContent}}
                    <option value="{{value}}">{{text}}</option>
                {{/levelContent}}
            </select>
        </td>
        <td id="{{costId}}" class="number">
        </td>
        <td>
            {{#spells}}
                <select id="{{id}}" class="field" tabindex="{{tabIndexValue}}">
                    {{#options}}
                        <option value="{{value}}">{{text}}</option>
                    {{/options}}
                </select>
            {{/spells}}
            {{^spells}}
                <input class="js-number field" id="{{id}}" size="3" tabindex="{{tabIndexValue}}"/>
            {{/spells}}
        </td>
        <td id="{{summaryId}}" class="number">
        </td>
        {{#subtractId}}
            <td>
               <input class="js-number field" id="{{subtractId}}" size="3" tabindex="{{tabIndexSubtract}}"/>
            </td>
        {{/subtractId}}
        {{#barracksTimes}}
            <td id="{{barrackQuantityId}}" class="number"></td>
        {{/barracksTimes}}
    </tr>