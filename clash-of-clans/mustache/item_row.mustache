    <tr id="{{rowId}}">
        <td>
            <span class="object object_{{objectType}}">
                <span class="object__icon object__icon_{{idDashed}}" title="{{title}}"></span>
                <span class="object__text">{{title}}</span>
            </span>
        </td>
        <td class="small">
            <select id="{{levelId}}" tabindex="{{tabIndexLevel}}" class="js-setting" data-component="level" data-type="{{objectType}}">
                {{#levelContent}}
                    <option value="{{value}}"{{#selected}} selected="selected"{{/selected}}>{{text}}</option>
                {{/levelContent}}
            </select>
        </td>
        <td>
            <input type="text"
                   pattern="[0-9]*"
                   data-type="{{objectType}}"
                   min="0"
                   max="999"
                   class="js-number text-middle"
                   id="{{id}}"
                   size="3"
                   tabindex="{{tabIndexValue}}"
                   data-component="quantity"/>
        </td>
        <td id="{{summaryId}}" class="number small">
        </td>
        {{#subtractId}}
            <td>
                <input type="text"
                       pattern="[0-9]*"
                       data-type="{{objectType}}"
                       min="0"
                       max="999"
                       class="js-number text-middle"
                       id="{{subtractId}}"
                       size="3"
                       tabindex="{{tabIndexSubtract}}"
                       data-component="subtract"/>
            </td>
        {{/subtractId}}
        {{#barracksTimes}}
            <td id="{{barrackQuantityId}}" class="number"></td>
        {{/barracksTimes}}
        {{#spells}}
            <td></td>
        {{/spells}}
    </tr>