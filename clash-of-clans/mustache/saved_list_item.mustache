<div class="saved-list__item" id="saved-list-item-{{index}}">
    {{#hasCapacity}}
        <div class="saved-list__item-result">
            {{totalCapacity}} / {{armyCamps}}
        </div>
    {{/hasCapacity}}

    {{#hasUnits}}
        <table class="saved-list__table">
            {{#units}}
                <tr>
                    <td class="number">{{quantity}}</td>
                    <td>{{name}}</td>
                </tr>
            {{/units}}
        </table>
        <div class="saved-list__item-title"><span class="cost-elixir">{{totalCost}}</span></div>
    {{/hasUnits}}

    {{#hasDark}}
        <table class="saved-list__table">
            {{#dark}}
                <tr>
                    <td class="number">{{quantity}}</td>
                    <td>{{name}}</td>
                </tr>
            {{/dark}}
        </table>
        <div class="saved-list__item-title"><span class="cost-dark-elixir">{{darkCost}}</span>&nbsp;</div>
    {{/hasDark}}

    {{#hasSpells}}
        <div class="saved-list__item-result">
            {{spellsCapacity}} / {{spellsFactoryLevel}}
        </div>
        <table class="saved-list__table">
            {{#spells}}
                <tr>
                    <td class="number">{{quantity}}</td>
                    <td>{{name}}</td>
                </tr>
            {{/spells}}
        </table>
        <div class="saved-list__item-title"><span class="cost-gold">{{spellsCost}}</span></div>
    {{/hasSpells}}

    <div class="saved-list__actions">
        <span class="button js-saved-load" data-num="{{index}}" tabindex="{{tabIndexLoad}}" title="Current calculation will be lost">Load</span>
        <span class="button button_after js-saved-delete" data-num="{{index}}" tabindex="{{tabIndexDelete}}">Delete</span>
    </div>
</div>